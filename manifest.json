{
  "manifest_version": 3,
  "name": "No Corner Radius",
  "version": "1.0",
  "description": "Removes rounded corners from webpages.",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["remove-radius.css"],
      "js": ["enforce-radius.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
